package geometrie;

public class segment {
	
	//variable d'instance
	public Point debut; 
	public Point fin; 

	
	//constructeur
	
	public segment () {
		
		this.debut=new Point(0,0);
		this.fin= new Point (0,0);
	}
	
	public segment (Point debut, Point fin) {
		this.debut=debut;
		this.fin=fin;
	}
	
	//public segment (segment autreSegment ) {
		//this.debut=autreSegment.
	//}
	
	
	
	
	public Point getDebut() {
		return debut;
	}
	
	public Point getFin() {
		return fin;
	}
	
	
	//methodes
	
	//attributs
	
	//print.out A;
	//print.out B;

}
