package geometrie;

public class segment {
	
	public Point debut = new Point (0,0);
	public Point fin = new Point (0,0);

	
	//constructeur
	

	
	public segment (Point debut, Point fin) {
		this.debut=debut;
		this.fin=fin;
	}
	
	//public segment (segment autreSegment ) {
		//this.debut=autreSegment.
	//}
	
	
	
	
	public Point getDebut() {
		return debut;
	}
	
	public Point getFin() {
		return fin;
	}
	
	
	//methodes
	
	//attributs
	
	//print.out A;
	//print.out B;

}
