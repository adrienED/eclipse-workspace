package geometrie;

import java.util.Arrays;

public class TestSegment {
	

	
	public static void main (String [] args) {
		
		segment origine = new segment (new Point (0,0),new Point (0,0));
		Point P= new Point(0,0);
		Point d=origine.getDebut();
		
		if ((origine.getDebut()).getX() == P.getX() && d.getY()==P.getY())
			System.out.println("Test origine.getX()                     [ok]");
		else
			System.out.println("Test origine.getX()                     [erreur]");

		if (origine.getFin() == new Point (0,0))
			System.out.println("Test origine.getY()                     [ok]");
		else
			System.out.println("Test origine.getY()                     [erreur]");
	}

}
